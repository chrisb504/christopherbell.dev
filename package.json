        "@babel/preset-react": "^7.22.5",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^9.1.2",
        "bootstrap": "^5.3.0",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^8.2.0",
